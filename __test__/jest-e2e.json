{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageReporters": ["json"],
    "coverageDirectory": "coverage/e2e",
    "collectCoverageFrom": ["src/**"],
    "coveragePathIgnorePatterns": [
      ".module.ts$",
      ".spec.ts$",
      ".e2e-spec.ts$"
    ]
}